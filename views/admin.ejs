    <div class="container">
    
        

        <table class="content-table">
          <thead>
            <tr class="">
              <th scope="col" >Id</th>
              <th scope="col">Ramal</th>
              <th scope="col">Setor</th>
              <th scope="col">Funcionario</th>
              <th scope="col">Empresa</th>
              <th scope="col" >Ac√£o</th>
              
            </tr>
          </thead>


          <tbody>
            <% result.forEach((tcadastro) => { %>


           <tr class="active-row">
             <td><%= tcadastro.id %></td>
             <td><%= tcadastro.ramal %></td>
             <td><%= tcadastro.setor %></td>
             <td><%= tcadastro.funcionario %></td>
             <td><%= tcadastro.empresa %></td>
             <td id="action-colun">
              <a href="/edit/<%= tcadastro.id %>"><button type="button" class="button-edit"><i class="fas fa-edit"></i></button></a>
              <a href="/delete/<%= tcadastro.id %>" ><button type="button" class="button-delete"><i class="fas fa-trash"></i></button></a>
             </td>
           </tr>
         <% }) %>
          </tbody>
        </table>
