<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="/public/styles/preview.css">
    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css" integrity="sha512-YWzhKL2whUzgiheMoBFwW8CKV4qpHQAEuvilg9FAn5VJUDwKZZxkJNuGM4XkWuk94WCrrwslk8yWNGmY1EduTA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

    <title>Ramais</title>
  </head>

  <body>


    <nav>
      <img src="/public/image/cropped-LOGO-LOURENCÌ§O-FILHO-105x95.png" alt="CLF">
      
      <label class="logo">DTI RAMAIS</label>

      <ul>
          <li><a class= "active" href="/">Home</a></li>
          
      </ul>
    </nav>

  <div class="container">
     

    <table class="content-table">
      <thead>
        <tr class="">
          <th scope="col" >Id</th>
          <th scope="col">Ramal</th>
          <th scope="col">Setor</th>
          <th scope="col">Funcionario</th>
          <th scope="col">Empresa</th>
          
        </tr>
      </thead>


      <tbody>
        <% result.forEach((tcadastro) => { %>


       <tr class="active-row">
         <td><%= tcadastro.id %></td>
         <td><%= tcadastro.ramal %></td>
         <td><%= tcadastro.setor %></td>
         <td><%= tcadastro.funcionario %></td>
         <td><%= tcadastro.empresa %></td>
       </tr>
     <% }) %>
      </tbody>
    </table>
</div>


   <div class="container-button">
    <a href="/" ><button class="button-query">Consultar registro</button></a>
   </div>

    
  </body>
</html>